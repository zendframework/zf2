{
    "name": "zendframework/zend-eventmanager",
    "description": "Event Manager component for Zend Framework",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf3",
        "event manager"
    ],
    "homepage": "https://github.com/zendframework/zf2",
    "autoload": {
        "psr-4": {
            "Zend\\EventManager\\": "src"
        }
    },
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "zendframework/zend-servicemanager": ">=3.0,<4.0"
    },
    "suggest": {
        "zendframework/zend-servicemanager": "If you want integration with the service manager for lazy listeners"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "dev-develop": "3.1-dev"
        }
    }
}
